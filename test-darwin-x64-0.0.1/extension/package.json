{
  "name": "test",
  "displayName": "test",
  "description": "test",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "test.helloWorld",
        "title": "Hello World"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "libertyjs",
          "title": "LibertyJS",
          "icon": "$(bell)"
        }
      ]
    },
    "views": {
      "libertyjs": [
        {
          "id": "speakers",
          "name": "Speakers"
        },
        {
          "id": "team",
          "name": "Organizers"
        },
        {
          "id": "details",
          "name": "Details",
          "type": "webview"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "speakers",
        "contents": "No speakers found"
      },
      {
        "view": "team",
        "contents": "No organizers found"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@types/vscode": "^1.83.0",
    "@types/webpack": "^5.28.3",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@vscode/vsce": "^2.21.1",
    "global": "^4.4.0",
    "lit": "^3.0.0"
  }
}
